@model GH.Web.Models.BookingClaimViewModel
<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
            $('#Product_Brand_sBrandName').autocomplete({
                source: '@Url.Action("Search2", "Brand")',
                minLength: 1,
                select: function (event, ui) {
                    $('#Product_Brand_sBrandName').val(ui.item.label);
                    $('#Product_kBrandId').val(ui.item.id);
                    return false;
                }
            });
        });
    });
</script>

@Html.HiddenFor(model => model.Product.kBrandId)


